{
  "name": "cerpus/content-author",
  "description": "Cerpus Content Author.",
  "keywords": [
    "content author"
  ],
  "license": "GPL-3.0",
  "type": "project",
  "require": {
    "php": "^7.2",
    "ext-dom": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-memcached": "*",
    "ext-zip": "*",
    "biigle/laravel-cached-openstack": "^1.2",
    "cerpus/cerpushelper": "~1.4.0",
    "cerpus/coreclient": "^1.1",
    "cerpus/gdpr": "^0.5",
    "cerpus/imageservice-client": "^1.1",
    "cerpus/jwtsupportlib": "^0.3.2",
    "cerpus/laravel-auth": "^0.24",
    "cerpus/license-client": "^1.3",
    "cerpus/metadataservice-client": "^1.0",
    "cerpus/questionbank-client": "^0.2.0",
    "cerpus/re-content-client": "^0.0.5",
    "cerpus/version-client": "^1.2",
    "ckeditor/ckeditor": "^4.5",
    "components/font-awesome": "^4.6",
    "components/jquery": "1.9.*",
    "components/jqueryui": "^1.11",
    "daniel-km/simple-iso-639-3": "^0.1.1",
    "doctrine/dbal": "^2.5",
    "embed/embed": "^3.2",
    "facade/ignition": "^1.11",
    "fideloper/proxy": "^4.0",
    "guzzlehttp/guzzle": "~6.0",
    "h5p/h5p-core": "1.24.0",
    "h5p/h5p-editor": "1.24.1",
    "laravel/framework": "^6.0",
    "laravel/helpers": "^1.1",
    "laravel/horizon": "^3.0",
    "laravel/tinker": "^1.0",
    "laravelcollective/html": "^6.0",
    "league/flysystem-aws-s3-v3": "~1.0",
    "league/flysystem-ziparchive": "^1.0",
    "league/fractal": "^0.18.0",
    "lusitanian/oauth": "^0.6.1",
    "masterminds/html5": "^2.6",
    "matthiasmullie/minify": "^1.3",
    "nimbusoft/flysystem-openstack-swift": "^0.3.1",
    "phpseclib/phpseclib": "2.0",
    "predis/predis": "^1.1",
    "ralouphie/mimey": "^2.0",
    "ramsey/uuid": "^3.4",
    "respect/validation": "^1.1",
    "stolz/assets": "^0.1.3",
    "vinelab/bowler": "^0.5.6",
    "xemlock/htmlpurifier-html5": "^0.1.8"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.4",
    "fakerphp/faker": "^1.16",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": "^8.0",
    "symfony/css-selector": "^4.0"
  },
  "replace": {
    "robloach/component-installer": "*"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories",
      "vendor/h5p",
      "vendor/daniel-km/simple-iso-639-3/src"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ]
  },
  "config": {
    "component-dir": "public/assets",
    "optimize-autoloader": true,
    "secure-http": false,
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
