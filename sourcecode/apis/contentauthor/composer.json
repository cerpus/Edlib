{
    "name": "cerpus/content-author",
    "description": "Cerpus Content Author.",
    "keywords": [
        "content author"
    ],
    "license": "GPL-3.0-only",
    "type": "project",
    "require": {
        "php": "8.1.*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-oauth": "*",
        "ext-pdo": "*",
        "ext-zip": "*",
        "cerpus/cerpushelper": "^2.1",
        "cerpus/coreclient": "^1.4.0",
        "cerpus/edlib-resource-kit-laravel": "^0.6.0",
        "cerpus/imageservice-client": "^2.1",
        "cerpus/laravel-rabbitmq-pubsub": "^2.1",
        "cerpus/questionbank-client": "^2.3.1",
        "cerpus/version-client": "^1.3",
        "ckeditor/ckeditor": "^4.5",
        "daniel-km/simple-iso-639-3": "^0.1.1",
        "doctrine/dbal": "^2.5",
        "embed/embed": "^3.2",
        "firebase/php-jwt": "^6.3",
        "guzzlehttp/guzzle": "^7.4",
        "h5p/h5p-core": "^1.24",
        "h5p/h5p-editor": "dev-master#1ae19fdb80839b32dad3846d6b0a5c745f8f6187",
        "laravel/framework": "^9.17",
        "laravel/horizon": "^5.7",
        "laravel/tinker": "^2.6",
        "laravel/ui": "^3.3",
        "laravelcollective/html": "^6.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/flysystem-ziparchive": "^3.0",
        "league/fractal": "^0.20",
        "league/mime-type-detection": "^1.9",
        "masterminds/html5": "^2.7",
        "matthiasmullie/minify": "^1.3",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^4.2",
        "spatie/laravel-ignition": "^1.0",
        "symfony/cache": "^6.1",
        "xemlock/htmlpurifier-html5": "^0.1.8"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "fakerphp/faker": "^1.16",
        "mockery/mockery": "^1.0",
        "nunomaduro/larastan": "^2.0",
        "phpstan/phpstan": "^1.7",
        "phpunit/phpunit": "^10.0",
        "symfony/css-selector": "^4.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "vendor/h5p",
            "vendor/daniel-km/simple-iso-639-3/src"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
