{
  "name": "cerpus/content-author",
  "description": "Cerpus Content Author.",
  "keywords": [
    "content author"
  ],
  "license": "GPL-3.0-only",
  "type": "project",
  "require": {
    "php": "8.1.*",
    "ext-dom": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-memcached": "*",
    "ext-oauth": "*",
    "ext-zip": "*",
    "cerpus/cerpushelper": "^2.1",
    "cerpus/coreclient": "^1.4.0",
    "cerpus/edlib-resource-kit-laravel": "^0.3.1",
    "cerpus/imageservice-client": "^2.1",
    "cerpus/jwtsupportlib": "^0.3.2",
    "cerpus/laravel-auth": "^1.0",
    "cerpus/laravel-rabbitmq-pubsub": "^2.0.1",
    "cerpus/questionbank-client": "^2.1",
    "cerpus/version-client": "^1.3",
    "ckeditor/ckeditor": "^4.5",
    "components/font-awesome": "^4.6",
    "components/jquery": "1.9.*",
    "components/jqueryui": "^1.11",
    "daniel-km/simple-iso-639-3": "^0.1.1",
    "doctrine/dbal": "^2.5",
    "embed/embed": "^3.2",
    "facade/ignition": "^2.16",
    "fideloper/proxy": "^4.0",
    "guzzlehttp/guzzle": "^7.4",
    "h5p/h5p-core": "^1.24",
    "h5p/h5p-editor": "dev-master#1ae19fdb80839b32dad3846d6b0a5c745f8f6187",
    "laravel/framework": "^8.0",
    "laravel/helpers": "^1.1",
    "laravel/horizon": "^5.7",
    "laravel/tinker": "^2.6",
    "laravel/ui": "^3.3",
    "laravelcollective/html": "^6.0",
    "league/flysystem-aws-s3-v3": "~1.0",
    "league/flysystem-ziparchive": "^1.0",
    "league/fractal": "^0.18.0",
    "league/mime-type-detection": "^1.9",
    "lusitanian/oauth": "^0.8.11",
    "masterminds/html5": "^2.6",
    "matthiasmullie/minify": "^1.3",
    "phpseclib/phpseclib": "2.0",
    "predis/predis": "^1.1",
    "ramsey/uuid": "^4.2",
    "respect/validation": "^1.1",
    "stolz/assets": "^0.2.0",
    "xemlock/htmlpurifier-html5": "^0.1.8"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.10",
    "fakerphp/faker": "^1.16",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^9.5",
    "symfony/css-selector": "^4.0"
  },
  "replace": {
    "robloach/component-installer": "*"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "vendor/h5p",
      "vendor/daniel-km/simple-iso-639-3/src"
    ],
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ]
  },
  "config": {
    "component-dir": "public/assets",
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
