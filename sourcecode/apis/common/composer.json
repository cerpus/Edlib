{
    "type": "project",
    "keywords": [
        "framework",
        "laravel"
    ],
    "require": {
        "php": "8.1.*",
        "ext-openssl": "*",
        "cerpus/laravel-rabbitmq-pubsub": "^2.1",
        "fruitcake/laravel-cors": "^2.2",
        "guzzlehttp/guzzle": "^7.4.5",
        "laravel/framework": "^9.23",
        "laravel/sanctum": "^3.0.1",
        "laravel/tinker": "^2.7.2",
        "packbackbooks/lti-1p3-tool": "^5.2",
        "predis/predis": "^1.1.10",
        "web-token/jwt-checker": "^2.2.11",
        "web-token/jwt-core": "^2.2.11",
        "web-token/jwt-signature-algorithm-rsa": "^2.2.11"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12.3",
        "spatie/laravel-ignition": "^1.3.1",
        "fakerphp/faker": "^1.20",
        "laravel/sail": "^1.15.1",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^6.2.1",
        "phpunit/phpunit": "^9.5.21"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "conflict": {
        "packbackbooks/lti-1p3-tool": ">5.2.1"
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan ide-helper:generate -W",
            "@php artisan ide-helper:models -M",
            "@php artisan ide-helper:meta",
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "prefer-stable": true
}
