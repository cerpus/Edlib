{
    "type": "project",
    "require": {
        "php": "8.3.*",
        "ext-dom": "*",
        "ext-intl": "*",
        "cerpus/edlib-resource-kit": "0.7.x-dev",
        "cerpus/edlib-resource-kit-laravel": "0.7.x-dev",
        "guzzlehttp/guzzle": "^7.8.1",
        "http-interop/http-factory-guzzle": "^1.2",
        "laravel/framework": "^10.37.3",
        "laravel/scout": "^10.6.1",
        "laravel/socialite": "^5.11",
        "laravel/tinker": "^2.8.2",
        "laravel/vapor-cli": "^1.61",
        "laravel/vapor-core": "^2.33.2",
        "league/flysystem-path-prefixing": "^3.0",
        "meilisearch/meilisearch-php": "^1.5.0",
        "psr/clock": "^1.0",
        "socialiteproviders/auth0": "^4.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "cache/array-adapter": "^1.2",
        "fakerphp/faker": "^1.23.0",
        "larastan/larastan": "^2.7.0",
        "laravel/dusk": "^7.12.0",
        "laravel/telescope": "^4.17.3",
        "mockery/mockery": "^1.6.7",
        "nunomaduro/collision": "^7.10.0",
        "phpstan/phpstan": "^1.10.50",
        "phpunit/phpunit": "^10.5.3",
        "spatie/laravel-ignition": "^2.3.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        },
        "platform": {
            "ext-sockets": "8.3.99"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
